{% comment %} add {% endcomment %}
<div class="dom__mainContainer">
  <div class="dom__container" id="add">
    <div class="dom__wrapper">
      <div class="dom__inputBlock">
        <div class="dom__header">
          <p>This module takes two numbers and returns the sum of these numbers</p>
        </div>
          <span>
            <label for="sum1">Number1 interger</label>
            <input
              class="inputResult js-value1"
              id="sum1"
              type="number" onkeydown="return event.keyCode !== 69">
          </span>
          <span>
            <label for="sum2">Number2 interger</label>
            <input
              class="inputResult js-value2"
              id="sum2"
              type="number"onkeydown="return event.keyCode !== 69">
          </span>
        <span>
          <input class="inputResult js-submit" type="submit">
        </span>
      </div>
      <div class="dom__outputBlock js-op_block-add">
        <span>
          <label class="js-error">Result</label>
          <h5 class="inputResult js-result"></h5>
        </span>
      </div>
    </div>
  </div>
